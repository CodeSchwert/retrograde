{
  "name": "retrograde",
  "version": "1.0.0",
  "description": "Retrograde API",
  "main": "index.js",
  "author": "CodeSchwert@protonmail.com",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "rimraf dist && babel -d dist src && cp -r data dist/",
    "start": "yarn run build && node -r dotenv/config dist/server.js",
    "start:watch": "yarn run build && nodemon --watch src --exec yarn start",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "yarn run start:watch",
    "scrape": "node ./data/scrape.js"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.2.1",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "node-html-parser": "^1.3.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/jest": "^26.0.14",
    "jest": "^26.5.2",
    "nodemon": "^2.0.4"
  },
  "jest": {
    "verbose": true,
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "dist/"
    ]
  }
}
